<form method="post" action="{{ path('app_agenda_delete', {'id': event.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ event.id) }}">
    <button class="btn border-danger m-2">
        <i class="fa-solid fa-calendar-xmark"></i>
    </button>
</form>
